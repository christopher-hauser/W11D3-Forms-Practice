extends layout.pug
block content
    table
        thead
            h2 Existing Users
            th id
            th First Name
            th Last Name
            th Email
            th Age
            th Favorite Beatle
            th Likes Ice Cream

        tbody
            each user in users
                tr= user.id
                tr= user.firstName
                tr= user.lastName
                tr= user.email
                tr= user.age
                tr= user.favoriteBeatle
                tr= user.likesIceCream
